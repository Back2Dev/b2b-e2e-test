{
  "name": "b2b-e2e-test",
  "version": "1.0.0",
  "description": "End to end testing for attendance app",
  "scripts": {
    "cypress:open": "./node_modules/cypress/bin/cypress open",
    "cypress:open:shop": "./node_modules/cypress/bin/cypress open --config integrationFolder=tests/cypress/integration/shop",
    "cypress:run": "./node_modules/cypress/bin/cypress run",
    "cypress:record": "./node_modules/cypress/bin/cypress run --record --key 7f96e2e6-b3ad-4fdc-8b70-af7cfc31f7da"
  },
  "main": "index.js",
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Back2bikes/b2b-e2e-test.git"
  },
  "keywords": [
    "test",
    "e2e",
    "meteor",
    "cypress"
  ],
  "author": "Mike King",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Back2bikes/b2b-e2e-test/issues"
  },
  "homepage": "https://github.com/Back2bikes/b2b-e2e-test#readme",
  "devDependencies": {
    "cypress": "^3.6.0",
    "faker": "^4.1.0"
  }
}
